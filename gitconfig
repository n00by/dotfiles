[alias]
    prp = !git pull --rebase && git push
    ca = !git commit -a -v
    cam = !git commit -a -m
[commit]
    verbose = true
[user]
    email = stefan.kleindl@rise-world.com
    name = Stefan Kleindl
[push]
    default = simple
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
